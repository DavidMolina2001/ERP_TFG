Imports System.Reflection.Emit
Imports System.Windows.Forms

Partial Public Class LabelMultiIdioma

    Public Property TextoCatalan As String
    Public Property TextoCastellano As String


    Public Sub New()
        InitializeComponent()

    End Sub



    Public Function CambiarIdioma(idioma As String)
        If idioma = "C" Then
            Me.Text = TextoCatalan
        ElseIf idioma = "E" Then
            Me.Text = TextoCastellano
        End If
        Return Me.Text
    End Function
End Class
